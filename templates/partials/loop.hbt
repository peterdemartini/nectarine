{{#each collections.posts}}
  <li class="post-stub" >
    <a class="js-ajax-link"  title="{{this.title}} | {{meta_title}}" href="{{this.path}}">
      <h4 class="post-stub-title">{{this.title}}</h4>
      <time class="post-stub-date" datetime="{{this.date}}">Published {{dateFormat this.date}}</time>
    </a>
  </li>
{{/each}}
